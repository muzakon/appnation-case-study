  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "bun-types": "latest",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prisma": "^7.3.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,jsonc}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "module": "src/server.js"
